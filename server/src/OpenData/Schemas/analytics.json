{
    "type": "object",
    "additionalProperties": true,
    "$schema": "http://json-schema.org/draft-03/schema",
    "properties": {
        "type": {
            "type": "string",
            "required": true
        },
        "branding": {
            "type": "array",
            "required": false,
            "items": {
                "type": "string",
                "required": true
            }
        },
        "runtime": {
            "type": "object",
            "required": true,
            "additionalProperties": true,
            "properties": {
                "fml": {
                    "type": "string",
                    "required": false
                },
                "forge": {
                    "type": "string",
                    "required": false
                }
            }
        },
        "language": {
            "type": "string",
            "required": true
        },
        "locale": {
            "type": "string",
            "required": true
        },
        "minecraft": {
            "type": "string",
            "required": true
        },
        "timezone": {
            "type": "string",
            "required": true
        },
        "signatures": {
            "type": "array",
            "required": true,
            "items": {
                "type": "object",
                "required": true,
                "properties": {
                    "filename": {
                        "type": "string",
                        "required": true
                    },
                    "signature": {
                        "type": "string",
                        "required": true
                    }
                }
            }
        },
        "workTime": {
            "type": "integer",
            "required": true
        }
    }
}